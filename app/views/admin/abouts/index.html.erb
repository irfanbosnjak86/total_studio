<p id="notice"><%= notice %></p>

<h1>Listing Admin Abouts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @abouts.each do |about| %>
      <tr>
        <td><%= link_to 'Show', admin_about %></td>
        <td><%= link_to 'Edit', edit_admin_about_path(admin_about) %></td>
        <td><%= link_to 'Destroy', admin_about, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About', new_admin_about_path %>
