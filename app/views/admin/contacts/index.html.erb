<p id="notice"><%= notice %></p>

<h1>Listing Admin Contacts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |admin_contact| %>
      <tr>
        <td><%= link_to 'Show', admin_contact %></td>
        <td><%= link_to 'Edit', edit_admin_contact_path(admin_contact) %></td>
        <td><%= link_to 'Destroy', admin_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_admin_contact_path %>
