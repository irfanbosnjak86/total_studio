<!DOCTYPE html>
<html>
<head>
  <title>TotalStudio</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>



<div class="navbar navbar-default navbar-fixed-top" id="mojNav" role="navigation">
			<div class="navbar-header">
				<div class="container">
					<button class="navbar-toggle" data-target=".navbar-collapse" data-toggle="collapse" type="button" id="menibutton">
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
				
					</button>

			</div><!-- navbar-header -->		

			<div class="navbar-collapse collapse" id="navNav">
				<ul class="nav navbar-nav">
					<li>
						<%= link_to "Home", root_path %>	
					</li>
					<li>
						<%= link_to "Blog", posts_path %>
					</li>
					<li>
						<%= link_to "O nama", o_nama_path %>
					</li>
					<li>
						<a href="#">Kontakt</a>
					</li>
				</ul><!-- end navbar-nav -->
			</div><!-- navbar-inverse -->
	</div><!-- end navbar-container -->
</div><!-- end navbar -->
<%= yield %>

<!-- First try for the online version of jQuery
	<script src="http://code.jquery.com/jquery.js"></script>-->

</body>
</html>
